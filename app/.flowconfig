[ignore]
.*/node_modules/oboe/.*

[untyped]
.*/node_modules/react-leaflet/.*
.*/node_modules/react-select/.*

[libs]
./src/marketplace/flowtype/global-types.js

[options]
module.name_mapper.extension='mdx' -> 'empty/object'
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'
module.name_mapper.extension='pcss' -> 'empty/object'
module.name_mapper.extension='po' -> 'empty/object'
module.name_mapper='\$app' -> '<PROJECT_ROOT>'
module.name_mapper='\$auth' -> '<PROJECT_ROOT>/src/auth'
module.name_mapper='\$docs' -> '<PROJECT_ROOT>/src/docs'
module.name_mapper='\$newdocs' -> '<PROJECT_ROOT>/src/newdocs'
module.name_mapper='\$mp' -> '<PROJECT_ROOT>/src/marketplace'
module.name_mapper='\$userpages' -> '<PROJECT_ROOT>/src/userpages'
module.name_mapper='\$routes' -> '<PROJECT_ROOT>/src/routes'
module.name_mapper='\$shared' -> '<PROJECT_ROOT>/src/shared'
module.name_mapper='\$testUtils' -> '<PROJECT_ROOT>/test/test-utils'
module.name_mapper='\$utils' -> '<PROJECT_ROOT>/src/utils'
module.name_mapper='\$editor' -> '<PROJECT_ROOT>/src/editor'

# allow dynamic import()
module.ignore_non_literal_requires=true

[version]
0.96.0
