@import 'variables.pcss';

.keyList {
  margin-bottom: 2rem;
}

.singleKey {
  margin-top: 3rem;
}

@media (--md-down) {
  .singleKey {
    :global(.dropdown) {
      display: none;
    }
  }
}

@media (--sm-down) {
  .singleKey input {
    font-size: 14px;
  }
}

@media (max-width: 450px) {
  .singleKey input {
    font-size: 12px;
  }
}

@media (max-width: 350px) {
  .singleKey input {
    font-size: 10px;
  }
}
