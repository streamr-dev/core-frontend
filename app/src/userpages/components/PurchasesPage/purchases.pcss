.title {
  font-family: 'IBM Plex Sans', sans-serif;
  font-size: 16px;
  color: #323232;
  letter-spacing: 0;
  line-height: 20px;
  margin-top: 4px;
}

.owner {
  font-family: 'IBM Plex Sans', sans-serif;
  font-size: 12px;
  color: #A3A3A3;
  letter-spacing: 0;
  line-height: 20px;
}

.status {
  font-family: 'IBM Plex Sans', sans-serif;
  font-size: 12px;
  color: #323232;
  letter-spacing: 0;
  line-height: 20px;
}

.active {
  color: var(--greenGoLive);
}

.expired {
  color: #323232;
}

@media (--md-down) {
  :global(html) .corepageContentContainer {
    width: 100%;
    max-width: none;
    padding-right: 24px;
    padding-left: 24px;
    margin-top: 34px;
  }
}

@media (--sm-down) {
  :global(html) .corepageContentContainer {
    margin-top: 16px;
  }
}
