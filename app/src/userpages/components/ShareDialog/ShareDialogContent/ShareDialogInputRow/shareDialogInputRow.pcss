.container {
  align-items: end;
  margin-top: 2rem;
  display: grid;
  grid-template-columns: 1fr 2.5um;
  grid-column-gap: 1rem;
}

.input {
  width: 100%;
  margin-right: 1rem;
}

.button {
  width: 2.5um;
  height: 2.5um;
}

.plusIcon {
  height: 8px;
  width: 8px;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
