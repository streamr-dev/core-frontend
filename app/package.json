{
  "name": "marketplace",
  "version": "0.9.25",
  "description": "The UI of Streamr Marketplace",
  "main": "src/index.jsx",
  "scripts": {
    "start": "webpack-dev-server --hot",
    "build": "node --max_old_space_size=4096 ./node_modules/.bin/webpack",
    "test": "jest ./test/* ./src/userpages/tests/*",
    "flow": "flow",
    "flow-stop": "flow stop",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix",
    "stylelint": "stylelint src/**/*.css src/**/*.[ps]css",
    "stylelint-fix": "stylelint src/**/*.css src/**/*.[ps]css --fix",
    "pre-commit-eslint": "./pre-commit-eslint.sh",
    "pre-commit-stylelint": "./pre-commit-stylelint.sh"
  },
  "pre-commit": {
    "run": [
      "pre-commit-eslint",
      "pre-commit-stylelint"
    ],
    "colors": false
  },
  "author": "",
  "license": "UNLICENSED",
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "lines": 50
      },
      "./src/userpages/": {
        "lines": 10
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/shared/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\$app/(.*)$": "<rootDir>/$1",
      "\\$mp/(.*)$": "<rootDir>/src/marketplace/$1",
      "\\$shared/(.*)$": "<rootDir>/src/shared/$1",
      "\\$testUtils/(.*)$": "<rootDir>/test/test-utils/$1",
      "\\.(css|scss|pcss)$": "identity-obj-proxy",
      "\\.(png)$": "empty/object"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/test-utils/setupTests.js"
  },
