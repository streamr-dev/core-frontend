{
  "name": "marketplace",
  "version": "2.1.6",
  "description": "The Streamr Core App",
  "main": "src/index.jsx",
  "scripts": {
    "start": "webpack-dev-server --hot --host 0.0.0.0",
    "build": "node --max_old_space_size=4096 ./node_modules/.bin/webpack --progress --profile",
    "test": "NODE_ENV=test jest ./test/* ./src/shared/test/* ./src/userpages/tests/* ./src/editor/shared/tests/* ./src/editor/canvas/tests/*",
    "flow": "flow",
    "flow-stop": "flow stop",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix",
    "stylelint": "stylelint src/**/*.css src/**/*.[ps]css",
    "stylelint-fix": "stylelint src/**/*.css src/**/*.[ps]css --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "CHROMATIC_APP_CODE=exgpyxh1il5 chromatic test --exit-zero-on-changes",
    "clear-cache": "npm run clean-cache",
    "clean-cache": "rm -rf node_modules/.cache; rm -rf .cache; rm -rf dist; jest --clearCache; killall flow; rm -rf /tmp/flow"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.{pcss,scss,css}": [
      "stylelint"
    ]
  },
  "author": "",
  "license": "UNLICENSED",
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "lines": 34
      },
      "./src/userpages/": {
        "lines": 10
      },
      "./src/shared/": {
        "lines": 10
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|pcss)$": "identity-obj-proxy",
      "\\.(png)$": "empty/object",
      "\\$app/(.*)$": "<rootDir>/$1",
      "\\$auth/(.*)$": "<rootDir>/src/auth/$1",
      "\\$mp/(.*)$": "<rootDir>/src/marketplace/$1",
      "\\$userpages/(.*)$": "<rootDir>/src/userpages/$1",
      "\\$routes$": "<rootDir>/src/routes",
      "\\$routes/(.*)?$": "<rootDir>/src/routes/$1",
      "\\$shared/(.*)$": "<rootDir>/src/shared/$1",
      "\\$testUtils/(.*)$": "<rootDir>/test/test-utils/$1",
      "\\$utils/(.*)$": "<rootDir>/src/utils/$1",
      "\\$editor/(.*)$": "<rootDir>/src/editor/$1",
      "\\$docs/(.*)$": "<rootDir>/src/docs/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/test-utils/setupTests.js"
    ]
  },
