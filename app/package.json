{
  "name": "core-frontend",
  "version": "2.5.11",
  "description": "The Streamr Core App",
  "main": "src/index.jsx",
  "scripts": {
    "cypress": "cypress run --headless",
    "start": "webpack-dev-server --hot --host 0.0.0.0",
    "start:ci": "node --max_old_space_size=4096 ./node_modules/.bin/webpack-dev-server --host 0.0.0.0",
    "build": "node --max_old_space_size=4096 ./node_modules/.bin/webpack --progress --profile",
    "test": "NODE_ENV=test jest --verbose --useStderr --forceExit",
    "flow": "flow",
    "flow-stop": "flow stop",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix",
    "stylelint": "stylelint src/**/*.css src/**/*.[ps]css",
    "stylelint-fix": "stylelint src/**/*.css src/**/*.[ps]css --fix",
    "storybook": "start-storybook -p 6006",
    "build-index": "node -r esm ./scripts/docsSearchIndexer/indexer.js",
    "build-storybook": "node --max_old_space_size=4096 ./node_modules/.bin/build-storybook",
    "chromatic": "chromatic --project-token exgpyxh1il5 --exit-zero-on-changes",
    "clear-cache": "npm run clean-cache",
    "clean-cache": "rm -rf node_modules/.cache; rm -rf .cache; rm -rf dist; jest --clearCache; killall flow; rm -rf /tmp/flow",
    "cloc": "npx cloc --exclude-dir=node_modules,coverage,package-lock.json ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.{pcss,scss,css}": [
      "stylelint"
    ]
  },
  "author": "",
  "license": "UNLICENSED",
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 31
      }
    },
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(test).[jt]s?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!*.spec.{js,jsx}",
      "!*.stories.*"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/docs/content/",
      "<rootDir>/src/docs/scripts/",
      "<rootDir>/src/marketplace/components/deprecated",
      "<rootDir>/src/marketplace/modules/deprecated/",
      "/flowtype/",
      "/tests/"
    ],
    "moduleNameMapper": {
      "\\.svg$": "<rootDir>/src/userpages/tests/__mocks__/fileMock.js",
      "\\.(css|scss|pcss|po|md)$": "identity-obj-proxy",
      "\\.(png)$": "<rootDir>/scripts/emptyObject",
      "\\$config$": "<rootDir>/test/test-utils/testConfig.js",
      "\\$app/(.*)$": "<rootDir>/$1",
      "\\$auth/(.*)$": "<rootDir>/src/auth/$1",
      "\\$mp/(.*)$": "<rootDir>/src/marketplace/$1",
      "\\$userpages/(.*)$": "<rootDir>/src/userpages/$1",
      "\\$routes$": "<rootDir>/src/routes",
      "\\$routes/(.*)?$": "<rootDir>/src/routes/$1",
      "\\$shared/(.*)$": "<rootDir>/src/shared/$1",
      "\\$testUtils/(.*)$": "<rootDir>/test/test-utils/$1",
      "\\$utils/(.*)$": "<rootDir>/src/utils/$1",
      "\\$ui/(.*)$": "<rootDir>/src/shared/components/Ui/$1",
      "\\$docs/(.*)$": "<rootDir>/src/docs/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/test-utils/setupTests.js"
    ]
  },
